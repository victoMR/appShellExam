<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 192" width="192" height="192">
  <!-- Background -->
  <rect width="192" height="192" fill="#2c3e50" rx="24"/>
  
  <!-- Shopping Cart Icon -->
  <g transform="translate(32, 32)">
    <!-- Cart Body -->
    <path d="M20 30h100l-8 40H28z" fill="#3498db" stroke="#fff" stroke-width="2"/>
    
    <!-- Cart Handle -->
    <path d="M20 30h-8l-4-16h-8" fill="none" stroke="#fff" stroke-width="3" stroke-linecap="round"/>
    
    <!-- Cart Wheels -->
    <circle cx="40" cy="86" r="6" fill="#fff"/>
    <circle cx="100" cy="86" r="6" fill="#fff"/>
    
    <!-- University Symbol -->
    <g transform="translate(45, 40)">
      <!-- Building/University -->
      <rect x="10" y="15" width="30" height="20" fill="#fff"/>
      <!-- Columns -->
      <rect x="12" y="10" width="3" height="25" fill="#fff"/>
      <rect x="18" y="10" width="3" height="25" fill="#fff"/>
      <rect x="24" y="10" width="3" height="25" fill="#fff"/>
      <rect x="30" y="10" width="3" height="25" fill="#fff"/>
      <rect x="36" y="10" width="3" height="25" fill="#fff"/>
      <!-- Roof -->
      <polygon points="25,5 10,15 40,15" fill="#fff"/>
    </g>
  </g>
  
  <!-- Text -->
  <text x="96" y="170" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="16" font-weight="bold">TC</text>
</svg>
